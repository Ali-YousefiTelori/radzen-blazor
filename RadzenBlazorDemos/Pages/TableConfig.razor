
<RadzenTable>
    <RadzenTableRow>
        @for (var i = 0; i < cols; i++)
        {
            var col = i;
            <RadzenTableHeaderCell>
                @($"Column {col}")
            </RadzenTableHeaderCell>
        }
    </RadzenTableRow>
    @for (var i = 0; i < rows; i++)
    {
        var row = i;
        <RadzenTableRow>
            @for (var j = 0; j < cols; j++)
            {
                var cell = j;
                <RadzenTableCell>
                    @($"Cell {row}{cell}")
                </RadzenTableCell>
            }
        </RadzenTableRow>
    }
</RadzenTable>
@code{
    int rows = 10;
    int cols = 10;
}